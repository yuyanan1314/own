<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fast.user.dao.SysConfigDao">

	<resultMap id="BaseResultMap" type="com.fast.user.entity.SysConfig">
		<id column="id" property="id" />
		<result column="k" property="k" />
		<result column="v" property="v" />
		<result column="remark" property="remark" />
		<result column="kv_type" property="kvType" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="deleted" property="deleted" />
		<result column="version" property="version" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, k, v, remark, kv_type AS kvType, create_time AS createTime, update_time AS updateTime, deleted, version
    </sql>
</mapper>
